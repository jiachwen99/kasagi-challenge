{
  "name": "kasagi-challenge",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start:a": "ts-node ./src/challengeA.ts",
    "start:b": "ts-node ./src/challengeB.ts",
    "docker:build": "docker build -t kasagi-challenge .",
    "docker:run": "docker run --name kasagi-challenge-container -v \"%cd%/objects.txt:/usr/src/app/input/objects.txt\" -v \"%cd%/output:/usr/src/app/output\" kasagi-challenge",
    "docker:build-run": "pnpm run docker:build && pnpm run docker:run"
  },
  "devDependencies": {
    "@types/node": "^18.19.55",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
